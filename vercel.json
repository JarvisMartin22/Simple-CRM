{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "unsafe-none"
        },
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "unsafe-none"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "www.trygolly.com"
        }
      ],
      "destination": "https://trygolly.com/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/((?!api|_next|_static|_vercel|favicon.ico|robots.txt|.*\\.).*)",
      "destination": "/index.html"
    }
  ]
} 